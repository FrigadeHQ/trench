{
  "name": "trench-js",
  "version": "0.0.3",
  "description": "Trench JS client for browser and Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --minify --target=chrome58 --minify-identifiers=false > dist/trench.min.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frigadehq/trench.git"
  },
  "keywords": [
    "analytics",
    "trench"
  ],
  "author": "Frigade Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frigadehq/trench/issues"
  },
  "homepage": "https://github.com/frigadehq/trench/packages/analytics-plugin-trench#readme",
  "devDependencies": {
    "@types/jest": "29.5.13",
    "esbuild": "^0.24.0",
    "jest": "^29.5.0",
    "jest-config": "^29.3.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "6.1.3",
    "tslint-plugin-prettier": "2.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "analytics": "^0.8.14",
    "analytics-plugin-trench": "0.0.3"
  },
  "jest": {
    "rootDir": ".",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(t|j)sx*$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ]
  }
}
