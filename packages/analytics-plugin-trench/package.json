{
  "name": "analytics-plugin-trench",
  "version": "0.0.8",
  "description": "Trench plugin for Analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frigadehq/trench.git"
  },
  "keywords": [
    "analytics",
    "trench"
  ],
  "author": "Frigade Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frigadehq/trench/issues"
  },
  "homepage": "https://github.com/frigadehq/trench/packages/analytics-plugin-trench#readme",
  "devDependencies": {
    "@types/jest": "29.5.13",
    "@types/node-fetch": "^2.6.11",
    "dts-bundle-generator": "^9.5.1",
    "jest": "^29.5.0",
    "jest-config": "^29.3.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.5",
    "tslint": "6.1.3",
    "tslint-plugin-prettier": "2.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.6"
  },
  "jest": {
    "rootDir": ".",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(t|j)sx*$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ]
  }
}
